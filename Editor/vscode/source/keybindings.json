// 将键绑定放在此文件中以覆盖默认值
[
    {
        "key": "ctrl+k ctrl+b",
        "command": "extension.openInDefaultBrowser",
        "when": "editorLangId == html"
    },
    {
        "key": "ctrl+k ctrl+o",
        "command": "editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    }
]
